<!--/partials/modalContents/newApt.ejs -->
<div class="modal fade" id="newAptModal" tabindex="-1" role="dialog" aria-labelledby="newAptModalLabel"
  aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="newAptModalLabel">New Apartment</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">

        <form >
          Apartment Complex: <input id='complex' name='complex'>
          Floorplan Name: <input id= 'name' name='name'>
          Beds: <input id='beds' name='beds'>
          Baths: <input id='baths' name='baths'>
          Rent: $<input id='rent' name='rent'>
          Deposit: $<input id='deposit' name='deposit'>
          SQ. Ft: <input id='sqft' name='sqft'>

          <div id='newAptTagGroup' class="btn-group-toggle" data-toggle="buttons">
            <!--tags buttons generated here-->
          </div>
          <button type="button" class="btn btn-secondary" data-toggle="modal" data-target="#newTagModal" onclick="loadNewTagCreator('new')"> + </button>
        </form>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" onclick="createApartment()">Create</button>
      </div>
    </div>
  </div>
</div>