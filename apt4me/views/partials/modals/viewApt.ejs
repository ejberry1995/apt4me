<!--/partials/modals/viewApt.ejs -->
<div class="modal fade" id="viewAptModal" tabindex="-1" role="dialog" aria-labelledby="viewAptModalLabel"
  aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="viewAptModalLabel">View Apartment</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">

        <input id='indexView' name='indexView' hidden>

        <div class="input-group mb-3">
          <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">Apartment Complex</span>
          </div>
          <input type="text" id='complexView' name='complexView' class="form-control" aria-describedby="basic-addon1" readonly>
        </div>
        
        <div class="input-group mb-3">
          <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">Floorplan Name</span>
          </div>
          <input type="text" class="form-control" id= 'nameView' name='nameView' aria-describedby="basic-addon1" readonly>
        </div>
        
        <div class="input-group mb-3">
          <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">Beds</span>
          </div>
          <input type="text" class="form-control" id='bedsView' name='bedsView' aria-describedby="basic-addon1" readonly>
        </div>
        
        <div class="input-group mb-3">
          <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">Baths</span>
          </div>
          <input type="text" class="form-control" id='bathsView' name='bathsView' aria-describedby="basic-addon1" readonly>
        </div>

        <div class="input-group mb-3">
          <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">SQFT</span>
          </div>
          <input type="text" class="form-control" id='sqftView' name='sqftView' aria-describedby="basic-addon1" readonly>
        </div>

        <div class="input-group mb-3">
          <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">Rent</span>
          </div>
          <input type="text" class="form-control" id='rentView' name='rentView' aria-describedby="basic-addon1" readonly>
        </div>

        <div class="input-group mb-3">
          <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">Deposit</span>
          </div>
          <input type="text" class="form-control" id='depositView' name='depositView' aria-describedby="basic-addon1" readonly>
        </div>

        <div id='viewAptTagGroup' class="btn-group-toggle" data-toggle="buttons">
          <!--tags buttons generated here-->
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>