<!--/partials/modals/viewApt.ejs -->
<div class="modal fade" id="viewAptModal" tabindex="-1" role="dialog" aria-labelledby="viewAptModalLabel"
  aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="viewAptModalLabel">View Apartment</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        Apartment Complex: <input id='complexView' name='complexView' class='form-control-plaintext' readonly >
        Floorplan Name: <input id= 'nameView' name='nameView' class='form-control-plaintext' readonly>
        <input id='indexView' name='indexView' hidden>
        Beds: <input id='bedsView' name='bedsView' class='form-control-plaintext' readonly>
        Baths: <input id='bathsView' name='bathsView' class='form-control-plaintext' readonly>
        Rent: $<input id='rentView' name='rentView' class='form-control-plaintext' readonly>
        Deposit: $<input id='depositView' name='depositViewt' class='form-control-plaintext' readonly>
        SQ. Ft: <input id='sqftView' name='sqftView' class='form-control-plaintext' readonly>

        <div id='viewAptTagGroup' class="btn-group-toggle" data-toggle="buttons">
          <!--tags buttons generated here-->
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" onclick="updateApartment()">Save</button>
      </div>
    </div>
  </div>
</div>